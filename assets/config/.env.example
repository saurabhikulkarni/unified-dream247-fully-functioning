# Copy this file to .env.dev and .env.prod
# Replace all values with your actual credentials

# App Configuration
APP_NAME=Dream247
APP_VERSION=1.0.0
ENVIRONMENT=development

# Shopping App - Hygraph CMS (GraphQL)
HYGRAPH_ENDPOINT=your_hygraph_endpoint
HYGRAPH_TOKEN=your_hygraph_token

# Fantasy Gaming App - REST APIs
UserServerUrl=your_user_server_url
TeamsServerUrl=your_teams_server_url
MatchServerUrl=your_match_server_url
ContestServerUrl=your_contest_server_url
DepositServerUrl=your_deposit_server_url
WithdrawServerUrl=your_withdraw_server_url
JoinContestServerUrl=your_join_contest_server_url
LiveMatchServerUrl=your_live_match_server_url
MyJoinContestServerUrl=your_my_join_contest_server_url
MyTeamsServerUrl=your_my_teams_server_url
CompletedMatchServerUrl=your_completed_match_server_url
OtherApiServerUrl=your_other_api_server_url
KycServerUrl=your_kyc_server_url
LeaderboardServerUrl=your_leaderboard_server_url

# Razorpay
RAZORPAY_KEY_ID=your_razorpay_key_id
RAZORPAY_KEY_SECRET=your_razorpay_key_secret

# Shiprocket
SHIPROCKET_EMAIL=your_shiprocket_email
SHIPROCKET_PASSWORD=your_shiprocket_password
SHIPROCKET_CHANNEL_ID=your_shiprocket_channel_id

# MSG91 OTP Service
MSG91_AUTH_KEY=your_msg91_auth_key

# Firebase
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_APP_ID=your_firebase_app_id
FIREBASE_MESSAGING_SENDER_ID=your_firebase_messaging_sender_id
FIREBASE_PROJECT_ID=your_firebase_project_id

# App Features
ENABLE_ANALYTICS=false
ENABLE_CRASHLYTICS=false
ENABLE_DEBUG_LOGS=true
